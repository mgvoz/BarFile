t\t\tuser?.result?.googleId === i?.creator ||\n\t\t\tuser?.result?._id === i?.creator,\n\t);\n\n\t//get current user's settings\n\tconst thisUsersSettings = settings.filter(\n\t\t(s) =>\n\t\t\tuser?.result?.googleId === s?.creator ||\n\t\t\tuser?.result?._id === s?.creator,\n\t);\n\n\t//get individual distributers and categories for drop-down menu\n\tconst distributers = thisUsersSettings[0]?.distributers[0]?.split(', ');\n\tconst categories = thisUsersSettings[0]?.categories[0]?.split(', ');\n\n\t//access camera on device and stream\n\tconst getVideoStream = () => {\n\t\tnavigator.mediaDevices\n\t\t\t.getUserMedia({\n\t\t\t\tvideo: { facingMode: 'environment' },\n\t\t\t})\n\t\t\t.then((stream) => {\n\t\t\t\tdocument.getElementById('video').srcObject = stream;\n\t\t\t})\n\t\t\t.catch((error) => console.log(error));\n\t};\n\n\t//scan barcode from live camera stream\n\tlet code;\n\tconst findBarcode = () => {\n\t\tif (\n\t\t\tBoolean(window.BarcodeDetector) === true &&\n\t\t\tdocument.getElementById('video')\n\t\t) {\n\t\t\tvar barcodeDetector = new window.BarcodeDetector({\n\t\t\t\tformats: [\n\t\t\t\t\t'ean_13',\n\t\t\t\t\t'ean_8',\n\t\t\t\t\t'upc_a',\n\t\t\t\t\t'upc_e',\n\t\t\t\t\t'code_128',\n\t\t\t\t\t'code_39',\n\t\t\t\t\t'code_93',\n\t\t\t\t\t'codabar',\n\t\t\t\t\t'itf',\n\t\t\t\t],\n\t\t\t});\n\n\t\t\tbarcodeDetector\n\t\t\t\t.detect(document.getElementById('video'))\n\t\t\t\t.then((data) => {\n\t\t\t\t\tif (data.length === 1) {\n\t\t\t\t\t\tcode = data[0].rawValue;\n\t\t\t\t\t\tapiLookup(code);\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t\t.catch((e) => {\n\t\t\t\t\tconsole.log(e);\n\t\t\t\t});\n\t\t}\n\t};\n\n\t//api barcode lookup\n\tlet key = '91990a226f804b428cb02a40c9f5b1d9';\n\tconst apiLookup = (code) => {\n\t\tconst url =\n\t\t\t'https://mgcorsproxy.herokuapp.com/https://api.apigenius.io/products/lookup?upc=' +\n\t\t\tcode +\n\t\t\t'&api_key=' +\n\t\t\tkey;\n\t\tfetch(url)\n\t\t\t.then((res) => {\n\t\t\t\tif (!res.ok) {\n\t\t\t\t\tconsole.log('failure');\n\t\t\t\t}\n\t\t\t\treturn res.json();\n\t\t\t})\n\t\t\t.then((data) => {\n\t\t\t\tif (data) {\n\t\t\t\t\tsetItemData({\n\t\t\t\t\t\t...itemData,\n\t\t\t\t\t\tnameOfItem: data.items.title,\n\t\t\t\t\t\timage: data.items.images[0],\n\t\t\t\t\t\tbarcode: data.items.upc,\n\t\t\t\t\t});\n\t\t\t\t\tscannedItems.push(itemData);\n\t\t\t\t}\n\t\t\t})\n\t\t\t.catch((err) => console.log(err));\n\t};\n\n\tsetInterval(findBarcode, 4000);\n\n\t//get info for selected scanned item\n\tconst currentItemInfo = scannedItems.filter(\n\t\t(i) => i === currentItem.nameOfItem,\n\t);\n\n\t//slider\n\n\t// submit form\n\tconst saveItemData = (e) => {\n\t\te.preventDefault();\n\t\tif (thisUsersItems.length > 0) {\n\t\t\tconst match = thisUsersItems.filter(\n\t\t\t\t(item) => item.barcode === itemData.barcode,\n\t\t\t);\n\t\t\tif (match.length !== 0) {\n\t\t\t\tdispatch(editItem(match[0]._id, { ...itemData }));\n\t\t\t\talert('Item saved successfully.');\n\t\t\t\thistory.push('/inventory');\n\t\t\t} else {\n\t\t\t\tdispatch(saveItem({ itemData }));\n\t\t\t\talert('Item saved successfully.');\n\t\t\t\thistory.push('/inventory');\n\t\t\t}\n\t\t} else {\n\t\t\tdispatch(saveItem({ itemData }));\n\t\t\talert('Item saved successfully.');\n\t\t\thistory.push('/inventory');\n\t\t}\n\t};\n\n\tconsole.log('scannedItems: ', scannedItems);\n\tconsole.log('itemData: ', itemData);\n\tconsole.log('currentItemInfo: ', currentItemInfo);\n\tconsole.log('currentItem: ', currentItem);\n\n\treturn (\n\t\t<>\n\t\t\t{loading === false ? (\n\t\t\t\t<>\n\t\t\t\t\t{width >= 1000 ? (\n\t\t\t\t\t\t<div className='inventory-container'>\n\t\t\t\t\t\t\t<div className='row'>\n\t\t\t\t\t\t\t\t<div id='nav-section' className='col-3'>\n\t\t\t\t\t\t\t\t\t<Navbar />\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div className='col-9'>\n\t\t\t\t\t\t\t\t\t<h1 className='dash-heading'>\n\t\t\t\t\t\t\t\t\t\tTake Inventory\n\t\t\t\t\t\t\t\t\t</h1>\n\t\t\t\t\t\t\t\t\t<hr className='dash-line' />\n\t\t\t\t\t\t\t\t\t<div className='take-inventory'>\n\t\t\t\t\t\t\t\t\t\t{Boolean(window.BarcodeDetector) ===\n\t\t\t\t\t\t\t\t\t\tfalse ? (\n\t\t\t\t\t\t\t\t\t\t\t<p id='message'>\n\t\t\t\t\t\t\t\t\t\t\t\tBarcode detection is not\n\t\t\t\t\t\t\t\t\t\t\t\tsupported in your browser. To\n\t\t\t\t\t\t\t\t\t\t\t\tmanually enter inventory,{' '}\n\t\t\t\t\t\t\t\t\t\t\t\t<Link\n\t\t\t\t\t\t\t\t\t\t\t\t\tto='/manual-entry'\n\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='manual-link-force'\n\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\tclick here.\n\t\t\t\t\t\t\t\t\t\t\t\t</Link>\n\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t\t<>\n\t\t\t\t\t\t\t\t\t\t\t\t<h3>\n\t\t\t\t\t\t\t\t\t\t\t\t\tBegin scanning below, or{' '}\n\t\t\t\t\t\t\t\t\t\t\t\t\t<Link\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tto='/manual-entry'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='manual-link'\n\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tclick here\n\t\t\t\t\t\t\t\t\t\t\t\t\t</Link>{' '}\n\t\t\t\t\t\t\t\t\t\t\t\t\tif you'd like to enter\n\t\t\t\t\t\t\t\t\t\t\t\t\tinventory manually.\n\t\t\t\t\t\t\t\t\t\t\t\t</h3>\n\t\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings.length ===\n\t\t\t\t\t\t\t\t\t\t\t\t0 ? (\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='inventory-settings'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<b>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tAttention! Please\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tset your\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdistributers,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tthreshold, and\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcategories on the\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSettings page before\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\ttaking inventory.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</b>\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t) : null}\n\t\t\t\t\t\t\t\t\t\t\t\t<p className='barcode-p'>\n\t\t\t\t\t\t\t\t\t\t\t\t\tScan barcode:\n\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t<center>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<video\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tid='video'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='video-view'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tmaxHeight='400px'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\twidth='30%'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tautoPlay\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tonCanPlay={getVideoStream()}\n\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tVideo stream not\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tavailable.\n\t\t\t\t\t\t\t\t\t\t\t\t\t</video>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='barcode-p'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tScanned item(s):\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<div className='item-info'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<ol>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{scannedItems.length >\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t0\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t? scannedItems.map(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\titem,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) => (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='info-text'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tonClick={setCurrentItem(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\titem,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{'  ' +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\titem.nameOfItem}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t),\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  )\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t: null}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</ol>\n\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<button\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='clear-scans'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tonClick={() =>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(scannedItems = [])\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tClear Scanned Items\n\t\t\t\t\t\t\t\t\t\t\t\t\t</button>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<br />\n\t\t\t\t\t\t\t\t\t\t\t\t\t<div className='set-amt'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tClick on a scanned item\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tabove, and use the\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tslider below to\n\t\t\t\t\t\t\t\t\t\t\t\t\t\trepresent quantity\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tremaining. If the\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tquantity remaining is at\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tor below your set\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tthreshold, this item\n\t\t\t\t\t\t\t\t\t\t\t\t\t\twill be added to you\n\t\t\t\t\t\t\t\t\t\t\t\t\t\torder list.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<div className='btl-img'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{currentItemInfo ? (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<img\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tsrc={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcurrentItemInfo?.image\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\talt='Selected item image'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tNo items\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tyet.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<form\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tonSubmit={saveItemData}\n\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='quantity'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tQuantity:{' '}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<b>{''}</b>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='threshold-info'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings.length ===\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t0\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t? 'Please set your desired distributers, threshold, and categories on the Settings page.'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t: 'Your threshold is currently set to ' +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  thisUsersSettings[0]\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t.threshold +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  '.'}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tWhich distributer do\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tyou typically order\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tthis product from?\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<select\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tname='distributer'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tid='distributer-select'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tonChange={(e) =>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tsetItemData({\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t...itemData,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdistributer:\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\te.target\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t.value,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t})\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tselected\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdisabled\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSelect\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tDistributer\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings[0]\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t?.length ===\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t0 ? (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdisabled\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tNo\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tDistributers\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tAvailable -\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSet your\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tDistributers\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\ton the\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSettings\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tPage.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdistributers?.map(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdist,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) => (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tvalue={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdist\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdist\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t),\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<br />\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tWhich category does\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tthis product belong\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tto?\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<select\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tname='category'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tid='category-select'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tonChange={(e) =>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tsetItemData({\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t...itemData,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcategory:\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\te.target\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t.value,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t})\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tselected\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdisabled\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSelect Category\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings[0]\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t?.length ===\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t0 ? (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdisabled\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tNo\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tCategories\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tAvailable -\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSet your\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tCategories\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\ton the\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSettings\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tPage.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcategories?.map(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcat,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) => (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tvalue={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcat\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcat\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t),\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<br />\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<button\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='save-quantity'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\ttype='submit'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSave This Item\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</button>\n\t\t\t\t\t\t\t\t\t\t\t\t\t</form>\n\t\t\t\t\t\t\t\t\t\t\t\t</center>\n\t\t\t\t\t\t\t\t\t\t\t</>\n\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t) : (\n\t\t\t\t\t\t<>\n\t\t\t\t\t\t\t<Navbar />\n\t\t\t\t\t\t\t<div className='inventory-container-m'>\n\t\t\t\t\t\t\t\t<h1 className='dash-heading-m'>\n\t\t\t\t\t\t\t\t\tTake Inventory\n\t\t\t\t\t\t\t\t</h1>\n\t\t\t\t\t\t\t\t<hr className='dash-line-m' />\n\t\t\t\t\t\t\t\t<div className='take-inventory'>\n\t\t\t\t\t\t\t\t\t{Boolean(window.BarcodeDetector) ===\n\t\t\t\t\t\t\t\t\tfalse ? (\n\t\t\t\t\t\t\t\t\t\t<p id='message'>\n\t\t\t\t\t\t\t\t\t\t\tBarcode detection is not supported\n\t\t\t\t\t\t\t\t\t\t\tin your browser. To manually enter\n\t\t\t\t\t\t\t\t\t\t\tinventory,{' '}\n\t\t\t\t\t\t\t\t\t\t\t<Link\n\t\t\t\t\t\t\t\t\t\t\t\tto='/manual-entry'\n\t\t\t\t\t\t\t\t\t\t\t\tclassName='manual-link-force'\n\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\tclick here.\n\t\t\t\t\t\t\t\t\t\t\t</Link>\n\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t<>\n\t\t\t\t\t\t\t\t\t\t\t<h3>\n\t\t\t\t\t\t\t\t\t\t\t\tBegin scanning below, or{' '}\n\t\t\t\t\t\t\t\t\t\t\t\t<Link\n\t\t\t\t\t\t\t\t\t\t\t\t\tto='/manual-entry'\n\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='manual-link'\n\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\tclick here\n\t\t\t\t\t\t\t\t\t\t\t\t</Link>{' '}\n\t\t\t\t\t\t\t\t\t\t\t\tif you'd like to enter inventory\n\t\t\t\t\t\t\t\t\t\t\t\tmanually.\n\t\t\t\t\t\t\t\t\t\t\t</h3>\n\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings.length === 0 ? (\n\t\t\t\t\t\t\t\t\t\t\t\t<center>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='inventory-settings'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<b>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tAttention! Please\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tset your\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdistributers,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tthreshold, and\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcategories on the\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSettings page before\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\ttaking inventory.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</b>\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t</center>\n\t\t\t\t\t\t\t\t\t\t\t) : null}\n\t\t\t\t\t\t\t\t\t\t\t<p className='barcode-p'>\n\t\t\t\t\t\t\t\t\t\t\t\tScan barcode:\n\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t<center>\n\t\t\t\t\t\t\t\t\t\t\t\t<video\n\t\t\t\t\t\t\t\t\t\t\t\t\tid='video'\n\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='video-view'\n\t\t\t\t\t\t\t\t\t\t\t\t\theight='300px'\n\t\t\t\t\t\t\t\t\t\t\t\t\twidth='90%'\n\t\t\t\t\t\t\t\t\t\t\t\t\tautoPlay\n\t\t\t\t\t\t\t\t\t\t\t\t\tplaysInline\n\t\t\t\t\t\t\t\t\t\t\t\t\tonCanPlay={getVideoStream()}\n\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\tVideo stream not available.\n\t\t\t\t\t\t\t\t\t\t\t\t</video>\n\t\t\t\t\t\t\t\t\t\t\t\t<p className='barcode-p'>\n\t\t\t\t\t\t\t\t\t\t\t\t\tScanned item(s):\n\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t<div className='item-info'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<ol>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t{scannedItems.length > 0\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t? scannedItems.map(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\titem,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t) => (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<li\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='info-text'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tonClick={setCurrentItem(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\titem,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{'  ' +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\titem.nameOfItem}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t),\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  )\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t: null}\n\t\t\t\t\t\t\t\t\t\t\t\t\t</ol>\n\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t<button\n\t\t\t\t\t\t\t\t\t\t\t\t\tclassName='clear-scans-m'\n\t\t\t\t\t\t\t\t\t\t\t\t\tonClick={() =>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t(scannedItems = [])\n\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\tClear Scanned Items\n\t\t\t\t\t\t\t\t\t\t\t\t</button>\n\t\t\t\t\t\t\t\t\t\t\t\t<div className='set-amt-m'>\n\t\t\t\t\t\t\t\t\t\t\t\t\tClick on a scanned item\n\t\t\t\t\t\t\t\t\t\t\t\t\tabove, and use the slider\n\t\t\t\t\t\t\t\t\t\t\t\t\tbelow to represent quantity\n\t\t\t\t\t\t\t\t\t\t\t\t\tremaining. If the quantity\n\t\t\t\t\t\t\t\t\t\t\t\t\tremaining is at or below\n\t\t\t\t\t\t\t\t\t\t\t\t\tyour set threshold, this\n\t\t\t\t\t\t\t\t\t\t\t\t\titem will be added to you\n\t\t\t\t\t\t\t\t\t\t\t\t\torder list.\n\t\t\t\t\t\t\t\t\t\t\t\t\t<div className='btl-img'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t{currentItemInfo ? (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<img\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tsrc={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tcurrentItemInfo?.image\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\talt='Selected item image'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<p>No items yet.</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t\t\t\t\t<form onSubmit={saveItemData}>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='quantity'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tQuantity: <b>{''}</b>\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p className='threshold-info-m'>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings.length ===\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t0\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t? 'Please set your desired distributers, threshold, and categories on the Settings page.'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t: 'Your threshold is currently set to ' +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  thisUsersSettings[0]\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t.threshold +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  '.'}\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tWhich distributer do you\n\t\t\t\t\t\t\t\t\t\t\t\t\t\ttypically order this\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tproduct from?\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<select\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tname='distributer'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tid='distributer-select-m'\n\t\t\t\t\t\t\t\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tonChange={(e) =>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tsetItemData({\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t...itemData,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdistributer:\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\te.target\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t.value,\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t})\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tselected\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdisabled\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tSelect Distributer\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t{thisUsersSettings[0]\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t?.length === 0 ? (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option disabled>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tNo Distributers\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tAvailable - Set\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tyour\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tDistributers on\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tthe Settings\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tPage.\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdistributers?.map(\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t(dist, key) => (\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<option\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tkey\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tvalue={\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tdist\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t{dist}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t),\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t)}\n\t\t\t\t\t\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tWhich category does this\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tproduct belong to?\n\t\t\t\t\t\t\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<select\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tname='category'\n\t\t\t